{
    "name": "axios-snippets",
    "displayName": "Axios Snippets",
    "description": "Axios Snippets axios代码片段补全，支持在.vue.js.html.ts中使用\n使用方式，直接在vscode插件市场中搜索安装",
    "version": "0.0.3",
    "publisher": "presidentma",
    "engines": {
        "vscode": "^1.24.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/presidentma/vscode-axios-snippets.git"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "javascript",
        "snippet",
        "axios",
        "vue axios"
    ],
    "activationEvents": [
        "onCommand:"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [],
        "snippets": [
            {
                "language": "javascript",
                "path": "./src/snippets/javascript.json"
            },
              {
                "language": "javascriptreact",
                "path": "./src/snippets/javascript.json"
            },
            {
                "language": "vue",
                "path": "./src/snippets/javascript.json"
            },
            {
                "language": "typescript",
                "path": "./src/snippets/javascript.json"
            },
            {
                "language": "html",
                "path": "./src/snippets/javascript.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
